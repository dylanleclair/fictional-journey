<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.HomeGUIPatientController">
   <children>
      <BorderPane prefHeight="600.0" prefWidth="900.0">
         <left>
            <VBox prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <children>
                  <VBox prefHeight="144.0" prefWidth="200.0">
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                     <children>
                        <HBox prefHeight="100.0" prefWidth="200.0">
                           <children>
                              <Label prefHeight="17.0" prefWidth="77.0" text="Appointments">
                                 <HBox.margin>
                                    <Insets left="20.0" />
                                 </HBox.margin>
                              </Label>
                              <Button fx:id="appointmentPatientButton" mnemonicParsing="false" onAction="#changeAppointmentButton" text="+">
                                 <HBox.margin>
                                    <Insets left="78.0" />
                                 </HBox.margin>
                              </Button>
                           </children>
                        </HBox>
                        <ListView prefHeight="200.0" prefWidth="200.0" />
                     </children>
                  </VBox>
                  <VBox prefHeight="144.0" prefWidth="100.0">
                     <children>
                        <HBox prefHeight="16.0" prefWidth="200.0">
                           <children>
                              <Label text="Tests">
                                 <HBox.margin>
                                    <Insets left="20.0" />
                                 </HBox.margin>
                              </Label>
                              <Button fx:id="testPatientButton" mnemonicParsing="false" onAction="#changeTestButton" text="+">
                                 <HBox.margin>
                                    <Insets left="128.0" />
                                 </HBox.margin>
                              </Button>
                           </children>
                           <VBox.margin>
                              <Insets />
                           </VBox.margin>
                        </HBox>
                        <ListView prefHeight="200.0" prefWidth="200.0" />
                     </children>
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </VBox>
               </children>
            </VBox>
         </left>
         <top>
            <HBox prefHeight="70.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <children>
                  <VBox prefHeight="70.0" prefWidth="200.0">
                     <children>
                        <Label fx:id="patientDateLabel" text="First Name:">
                           <padding>
                              <Insets top="10.0" />
                           </padding>
                           <VBox.margin>
                              <Insets left="10.0" />
                           </VBox.margin>
                        </Label>
                        <Label fx:id="patientTimeLabel" text="Last Name:">
                           <VBox.margin>
                              <Insets left="10.0" top="10.0" />
                           </VBox.margin>
                        </Label>
                     </children>
                  </VBox>
               </children>
            </HBox>
         </top>
         <center>
            <TableView prefHeight="449.0" prefWidth="596.0" BorderPane.alignment="CENTER">
              <columns>
                <TableColumn fx:id="tableViewTimePatient" prefWidth="75.0" text="Time" />
                <TableColumn prefWidth="75.0" text="Monday" />
                  <TableColumn prefWidth="75.0" text="Tuesday" />
                  <TableColumn prefWidth="75.0" text="Wednesday" />
                  <TableColumn prefWidth="75.0" text="Thursday" />
                  <TableColumn prefWidth="75.0" text="Friday" />
                  <TableColumn prefWidth="75.0" text="Saturday" />
                  <TableColumn prefWidth="75.0" text="Sunday" />
              </columns>
               <BorderPane.margin>
                  <Insets bottom="30.0" right="30.0" />
               </BorderPane.margin>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </center>
      </BorderPane>
   </children>
</AnchorPane>
